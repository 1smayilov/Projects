{
  "TokenOptions": {
    "Audience": "elvin@elvin.com", // "Audience": "elvin@elvin.com" parametri token-in bu email adresinə məxsus olduğunu bildirir.
    "Issuer": "elvin@elvin.com", // "Issuer": "elvin@elvin.com" parametri token-in bu email adresindən yaradıldığını bildirir.
    "AccessTokenExpiration": 10, // Verdiyimiz token inin keçərlilik vaxtı
    "SecurityKey": "mysupersecretkeymysupersecretkey" // Bu, token-in imzalanmasında istifadə olunan təhlükəsizlik açarıdır.
    // "SecurityKey": "mysupersecretkeymysupersecretkey" parametri token-in imzalanması üçün istifadə olunan gizli açarı təyin edir.

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
